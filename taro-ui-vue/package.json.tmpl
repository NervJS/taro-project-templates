{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "private": true,
  "description": "<%= description %>",
  "templateInfo": {
    "name": "<%= template %>",
    "typescript": <%= typescript %>,
    "css": "<%= css %>"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/cli": "<%= version %>",
    "@tarojs/components": "<%= version %>",
    "@tarojs/runtime": "<%= version %>",
    "@tarojs/taro": "<%= version %>",
    "lodash": "4.17.15",<% if (framework === 'react') {%>
    "@tarojs/react": "<%= version %>",
    "react": "^16.10.0",
    "react-dom": "^16.10.0",
    "taro-ui": "^3.0.0-alpha.3"<%}%><% if (framework === 'nerv') {%>
    "nervjs": "^1.5.0",
    "taro-ui": "^3.0.0-alpha.3"<%}%><% if (framework === 'vue') {%>
    "vue-template-compiler": "^2.5.0",
    "vue": "^2.5.0",
    "taro-ui-vue": "^1.0.0-alpha.5"<%}%>
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "<%= version %>",
    "@tarojs/webpack-runner": "<%= version %>",
    "@types/webpack-env": "^1.13.6",
    "babel-preset-taro": "<%= version %>",
    "eslint": "^6.8.0",<% if (framework === 'vue') {%>
    "vue-loader": "^15.9.2",
    "eslint-plugin-vue": "^6.x",<%}%><% if (framework === 'vue3') {%>
    "@vue/compiler-sfc": "^3.0.0-beta.15",
    "vue-loader": "^16.0.0-beta.3",
    "eslint-plugin-vue": "^7.0.0-alpha.6",<%}%>
    "eslint-config-taro": "<%= version %>",
    "stylelint": "9.3.0",<% if (typescript) {%>
    "@typescript-eslint/parser": "^2.x",
    "@typescript-eslint/eslint-plugin": "^2.x",
    "typescript": "^3.7.0",<%}%><% if (framework === 'react' || framework === 'nerv') {%>
    "@types/react": "^16.0.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react-hooks": "^1.6.1"<%}%>
  }
}
