{
  "name": "{{ projectName }}",
  "version": "1.0.0",
  "private": true,
  "description": "{{ description }}",
  "templateInfo": {
    "name": "{{ template }}",
    "typescript": {{ typescript }},
    "css": "{{ css }}",
    "framework": "{{ framework }}"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
